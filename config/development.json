{
  "app": {
    "maxParallelQueries": 5
  },
  "central": {
    "baseUrl": "https://endpoint.192.168.99.100/",
    "caFile": "./certs/ca.crt",
    "certFile": "./certs/client1-crt.pem",
    "keyFile": "./certs/client1-key.pem"
  },
  "db": {
    "database": "vault",
    "user": "postgres",
    "password": "postgres",
    "host": "localhost",
    "port": 5432,
    "max": 10,
    "idleTimeoutMillis": 30000
  },
  "log": {
    "console": {
      "level": "debug",
      "prettyPrint": true,
      "json": false,
      "colorize": true,
      "timestamp": true,
      "humanReadableUnhandledException": true
    },
    "file": {
      "level": "debug",
      "colorize": "true",
      "timestamp": "true",
      "filename": "logs/logfile.log",
      "maxsize": 5242880,
      "maxFiles": 20,
      "json": false,
      "prettyPrint": true,
      "tailable": true,
      "zippedArchive": true
    }
  }
}
